@page
@using gpconnect_appointment_checker.Helpers.Constants
@model NotRegisteredModel
@addTagHelper *, gpconnect-appointment-checker

<div class="nhsuk-grid-row">
    <div class="nhsuk-grid-column-full">
        <div class="nhsuk-warning-callout">
            <h3 class="nhsuk-warning-callout__label">@SearchConstants.AccessLabel</h3>
            <p>@SearchConstants.NotRegisteredText</p>
            <partial name="Pending/PracticeSignup" />
            <p>
                @Html.Raw(string.Format(SearchConstants.NotRegisteredLinkText, "/CreateAccount"))</p>
        </div>
    </div>
</div>